{
  "name": "hbplugin-auto-mute-discord",
  "packageManager": "yarn@3.3.0",
  "version": "1.0.0",
  "description": "My cool Hindenburg plugin",
  "keywords": [
    "hindenburg",
    "plugin",
    "among us"
  ],
  "license": "GPL-3.0-only",
  "files": [
    "dist",
    "config.schema.json"
  ],
  "main": "./dist/index.js",
  "types": "./index.ts",
  "publishConfig": {
    "types": "./dist/index.d.ts"
  },
  "scripts": {
    "publish": "yarn npm publish --access public",
    "build": "tsc --project ./",
    "watch": "tsc --watch --project ./",
    "prepack": "yarn build"
  },
  "engines": {
    "node": ">=14",
    "hindenburg": "1.1.*"
  },
  "devDependencies": {
    "@skeldjs/hindenburg": "^1.1.22",
    "@types/async": "^3",
    "typescript": "latest"
  },
  "plugin": {
    "loadOrder": "none",
    "defaultConfig": {
      "botTokenMain": "MAIN_TOKEN_HERE",
      "botTokenSecond": "SECOND_TOKEN HERE",
      "mariadbDatabase": "mutedb",
      "mariadbUser": "muteuser",
      "mariadbPassword": "mutepassword",
      "mariadbHost": "127.0.0.1",
      "mariadbPort": 3306
    }
  },
  "dependencies": {
    "@typeit/discord": "^5.0.16",
    "async": "^3.2.4",
    "discord.js": "^14.6.0",
    "mariadb": "^3.0.2"
  }
}
